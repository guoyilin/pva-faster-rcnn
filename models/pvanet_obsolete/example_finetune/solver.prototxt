train_net: "models/pvanet/example_finetune/train.prototxt"

base_lr: 0.0001
lr_policy: "step"
gamma: 0.1
stepsize: 50000
display: 20
average_loss: 100
momentum: 0.9
weight_decay: 0.0002


# We disable standard caffe solver snapshotting and implement our own snapshot
# function
snapshot: 0
# We still use the snapshot prefix, though
snapshot_prefix: "pvanet_frcnn"
iter_size: 2
